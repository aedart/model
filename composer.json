{
    "name": "aedart/model",
    "description": "Collection of getter- / setter-interfaces with various trait implementations, for different kinds of 'common' properties.",
    "keywords": [
        "Getter",
        "Setter",
        "Model",
        "Data",
        "Validation"
    ],
    "homepage": "https://bitbucket.org/aedart/model",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        },
        {
            "name": "Bartlomiej Szala",
            "email": "fenix440@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aedart/model-id": "1.*",
        "aedart/model-uuid": "1.*",
        "aedart/model-email": "1.*",
        "fenix440/model-description": "1.*",
        "fenix440/model-name": "1.*"
    },
    "require-dev": {
      "aedart/license": "1.*",
      "aedart/license-file-manager": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Model\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "suggest": {
        "aedart/overload": "Property overloading via PHP's magic methods __get, __set, __isset and __unset"
    },
    "scripts": {
      "post-update-cmd": "php vendor/bin/license-manager license:copy vendor/aedart/license/aedart/model/BSD-3-Clause"
    }
}
