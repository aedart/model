{
  "name": "aedart/model",
  "description": "Collection of getter- / setter-interfaces with various trait implementations, for different kinds of 'common' properties.",
  "keywords": [
    "Getter",
    "Setter",
    "Model",
    "DTO"
  ],
  "homepage": "https://github.com/aedart/model",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Alin Eugen Deac",
      "email": "aedart@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "aedart/license": "1.*",
    "aedart/license-file-manager": "~1.8",
    "aedart/testing-gst": "~2.0",
    "aedart/scaffold-bundle": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Aedart\\Model\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/testCases"
    ]
  },
  "suggest": {
    "aedart/scaffold": "console wizard for installing something...",
    "aedart/scaffold-bundle": "A few out of the box scaffolds",
    "aedart/dto": "This package is ideal for the Data Transfer Object (DTO) abstraction"
  },
  "scripts": {
    "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause --ansi"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
