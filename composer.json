{
    "name": "aedart/model",
    "description": "Collection of getter- / setter-interfaces with various trait implementations, for different kinds of 'common' properties.",
    "keywords": [
        "Getter",
        "Setter",
        "Model",
        "Data",
        "Validation"
    ],
    "homepage": "https://bitbucket.org/aedart/model",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        },
        {
            "name": "Bartlomiej Szala",
            "email": "fenix440@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
      "aedart/license": "1.*",
      "aedart/license-file-manager": "~1.3",
      "aedart/testing-laravel": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Model\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "suggest": {
        "aedart/dto": "This package is ideal for the Data Transfer Object (DTO) abstraction"
    },
    "scripts": {
      "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/model/BSD-3-Clause"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
