{
  "name": "aedart/model",
  "description": "Collection of getter- / setter-interfaces with various trait implementations, for different kinds of 'common' properties.",
  "keywords": [
    "Getter",
    "Setter",
    "Model",
    "DTO"
  ],
  "homepage": "https://github.com/aedart/model",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Alin Eugen Deac",
      "email": "aedart@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9"
  },
  "require-dev": {
    "aedart/license": "1.*",
    "aedart/license-file-manager": "~1.5",
    "aedart/testing-laravel": "1.7.*",
    "aedart/scaffold-bundle": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Aedart\\Model\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "suggest": {
    "aedart/dto": "This package is ideal for the Data Transfer Object (DTO) abstraction"
  },
  "scripts": {
    "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
